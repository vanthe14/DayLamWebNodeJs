  <% if(typeof paginator!="undefined"){ %>
<div class="span10 blog">
    <!-- Pagination -->
    <div class="pagination" id="pagination">
        <ul>
            <li class="" <% if (!paginator.previous) {%>style="display:none;" <% } %>><a href="<%=paginator.prelink%>?page=<%=parseInt(paginator.previous) + 1%>">Prev</a></li>
          <% for (var page=0; page<paginator.range.length; page++) { %>
            <li class=" <% if (paginator.range[page]==paginator.next) { %> active <% } %>"><a href="<%=paginator.prelink%>?page=<%=paginator.range[page]%>"><%=paginator.range[page]%></a></li>
            <%}%>
          <% if (paginator.next<paginator.pageCount) {%>
            <li class=""><a href="<%=paginator.prelink%>?page=<%=parseInt(paginator.next) + 1%>">Next</a></li>
          <% } %>
        </ul>
    </div>
</div>
<%} %>
